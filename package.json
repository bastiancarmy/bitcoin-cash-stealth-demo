{
  "name": "bch-stealth-demo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -Apt --topological run build",
    "clean": "yarn workspaces foreach -Apt --topological run clean",
    "typecheck": "yarn workspaces foreach -A -pt run typecheck",
    "bch-stealth": "yarn workspace @bch-stealth/cli exec bch-stealth",
    "bchctl": "node ./packages/cli/dist/index.js"
  },
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "electron-builder-squirrel-windows": "^26.8.1"
  }
}
